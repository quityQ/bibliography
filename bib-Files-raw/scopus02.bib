	type = {Conference paper},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 0; Conference name: 6th International Conference on Engineering Sciences, ICES 2022; Conference date: 18 December 2022 through 22 December 2022; Conference code: 199287}
}

