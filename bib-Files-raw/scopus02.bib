	author = {Potosky, Denise and Azan, Wilfrid},
	title = {Leadership behaviors and human agency in the valley of despair: A meta-framework for organizational change implementation},
	year = {2023},
	journal = {Human Resource Management Review},
	volume = {33},
	number = {1},
	doi = {10.1016/j.hrmr.2022.100927},
